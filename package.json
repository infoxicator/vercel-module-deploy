{
  "name": "production-module",
  "version": "1.0.0",
  "description": "",
  "contributors": [],
  "scripts": {
    "start": "one-app-runner",
    "prebuild": "npm run clean",
    "build": "bundle-module",
    "watch:build": "npm run build -- --watch",
    "clean": "rimraf build",
    "prepare": "npm run build",
    "test:lint": "eslint --ignore-path .gitignore --ext js,jsx,snap .",
    "test:unit": "jest",
    "test": "npm run test:lint && npm run test:unit"
  },
  "one-amex": {
    "app": {
      "compatibility": "^5.0.0"
    },
    "runner": {
      "dockerImage": "oneamex/one-app-dev:latest",
      "modules": [
        "."
      ],
      "rootModuleName": "production-module"
    },
    "bundler": {
      "providedExternals": [
        "react-intl"
      ]
    }
  },
  "dependencies": {
    "@americanexpress/one-app-ducks": "^4.0.0",
    "@americanexpress/one-app-router": "^1.0.0",
    "content-security-policy-builder": "^2.1.0",
    "fs-extra": "^9.0.1",
    "holocron": "^1.1.0",
    "immutable": "^3.8.2",
    "node-fetch": "^2.6.1",
    "prop-types": "^15.5.9",
    "react": "^16.12.0",
    "react-intl": "^4.5.3",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@americanexpress/one-app-bundler": "^6.0.0",
    "@americanexpress/one-app-runner": "^6.0.0",
    "amex-jest-preset-react": "^6.0.0",
    "babel-eslint": "^8.2.6",
    "babel-preset-amex": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-amex": "^11.1.0",
    "jest": "^25.1.0",
    "rimraf": "^3.0.0",
    "glob": "^7.1.6",
    "@babel/polyfill": "^7.8.3",
    "jest-json-schema": "^2.1.0"
  },
  "jest": {
    "preset": "amex-jest-preset-react",
    "setupFilesAfterEnv": [
      "./test-setup.js"
    ]
  }
}
